<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.att.vcc.inventorycollector.domain.NaradCable" schema="dti" table="narad_cable">
        <meta attribute="class-description">This class contains the narad cable detail.</meta>
	<meta attribute="use-in-tostring">true</meta>
	<composite-id>
        	<key-property name="physicallocationid" column="Physical_location_id"  type="string"/>
		<key-property name="cablename" column="cable_name" type="string" />
	</composite-id>
        <property name="cabletype" column="cable_type" type="string" />
        <property name="resourceversion" column="resource_version" type="string" />
        <property name="updatedOn" column="UPDATED_ON" type="string" />
     </class>
</hibernate-mapping>
