        <property name="isclosedloopdisabled" column="IS_CLOSED_LOOP_DISABLED" type="string" />
        <property name="groupnotation" column="group_notation" type="string" />
        <property name="modelinvariantid" column="MODEL_INVARIANT_ID" type="string" />
        <property name="modelversionid" column="MODEL_VERSION_ID" type="string" />
        <property name="modelCustomizationId" column="MODEL_CUSTOMIZATION_ID" type="string" />
        <property name="updatedOn" column="UPDATED_ON" type="string" />
