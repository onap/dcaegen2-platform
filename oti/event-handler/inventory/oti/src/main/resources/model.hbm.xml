<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.att.vcc.inventorycollector.domain.Model" schema="dti" table="rt_model">
        <meta attribute="class-description">This class contains the model detail.</meta>
        <meta attribute="use-in-tostring">true</meta>
        <id name="modelinvariantid" column="MODEL_INVARIANT_ID"  type="java.lang.String"/>
        <property name="modeltype" column="MODEL_TYPE" type="string" />
        <property name="resourceversion" column="RESOURCE_VERSION" type="string" />
        <property name="updatedOn" column="UPDATED_ON" type="string" />
    </class>
</hibernate-mapping>
