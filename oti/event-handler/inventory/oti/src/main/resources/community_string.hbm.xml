<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.att.vcc.inventorycollector.domain.CommunityString" schema="dti" table="community_string">
        <meta attribute="class-description">This class contains the community string detail.</meta>
        <meta attribute="use-in-tostring">true</meta>
        <composite-id>
            <key-property name="resourcetype" column="RESOURCE_TYPE" type="string"/>
            <key-property name="resourcename" column="RESOURCE_NAME" type="string"/>
        </composite-id>
        <property name="communitystring" column="COMMUNITY_STRING" type="string" />
        <property name="snmpversion" column="SNMP_VERSION" type="string" />
        <property name="reservationid" column="RESERVATION_ID" type="string" />
        <property name="updatedOn" column="UPDATED_ON" type="string" />
    </class>
</hibernate-mapping>
