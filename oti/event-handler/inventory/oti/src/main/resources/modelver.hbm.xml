<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.att.vcc.inventorycollector.domain.ModelVer" schema="dti" table="rt_model_ver">
        <meta attribute="class-description">This class contains the model version detail.</meta>
        <meta attribute="use-in-tostring">true</meta>
        <composite-id>
            <key-property name="modelversionid" column="MODEL_VERSION_ID" type="string"/>
            <key-property name="modelinvariantid" column="MODEL_INVARIANT_ID" type="string"/>
        </composite-id>
        <property name="modelname" column="MODEL_NAME" type="string" />
        <property name="modelversion" column="MODEL_VERSION" type="string" />
        <property name="modeldescription" column="MODEL_DESCRIPTION" type="string" />
        <property name="distributionstatus" column="DISTRIBUTION_STATUS" type="string" />
        <property name="resourceversion" column="RESOURCE_VERSION" type="string" />
        <property name="updatedOn" column="UPDATED_ON" type="string" />
    </class>
</hibernate-mapping>
