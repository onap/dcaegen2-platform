<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.att.vcc.inventorycollector.domain.Volume" schema="dti" table="rt_volume">
        <meta attribute="class-description">This class contains the volume detail.</meta>
        <meta attribute="use-in-tostring">true</meta>
        <composite-id>
		    <key-property name="volumeid" column="VOLUME_ID" type="string"/>
		    <key-property name="vserverid" column="VSERVER_ID" type="string"/>
		</composite-id>
        <property name="volumeselflink" column="VOLUME_SELFLINK" type="string" />
        <property name="resourceversion" column="RESOURCE_VERSION" type="string" />
        <property name="updatedOn" column="UPDATED_ON" type="string" />
    </class>
</hibernate-mapping>
