<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.att.vcc.inventorycollector.domain.NaradChassis" schema="dti" table="narad_chassis">
        <meta attribute="class-description">This class contains the narad chassis detail.</meta>
	<meta attribute="use-in-tostring">true</meta>
        <id name="chassisname" column="chassis_name" type="string" />
        <property name="chassistype" column="chassis_type" type="string" />
        <property name="chassisrole" column="chassis_role" type="string" />
        <property name="serialnumber" column="serial_number" type="string" />
        <property name="assettag" column="asset_tag" type="string" />
        <property name="resourceversion" column="resource_version" type="string" />
        <property name="updatedOn" column="UPDATED_ON" type="string" />
     </class>
</hibernate-mapping>
