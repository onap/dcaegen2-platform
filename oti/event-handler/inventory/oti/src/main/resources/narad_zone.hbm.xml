<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.att.vcc.inventorycollector.domain.NaradZone" schema="dti" table="narad_zone">
        <meta attribute="class-description">This class contains the narad zone detail.</meta>
        <meta attribute="use-in-tostring">true</meta>
        <id name="zoneid" column="ZONE_ID"  type="java.lang.String"/>
        <property name="zonename" column="ZONE_NAME" type="string" />
        <property name="designtype" column="DESIGN_TYPE" type="string" />
        <property name="zonecontext" column="ZONE_CONTEXT" type="string" />
        <property name="status" column="STATUS" type="string" />
        <property name="resourceversion" column="RESOURCE_VERSION" type="string" />
        <property name="inmaint" column="IN_MAINT" type="string" />
        <property name="updatedOn" column="UPDATED_ON" type="string" />
    </class>
</hibernate-mapping>
