<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.att.vcc.inventorycollector.domain.NaradRack" schema="dti" table="narad_rack">
        <meta attribute="class-description">This class contains the narad rack detail.</meta>
        <meta attribute="use-in-tostring">true</meta>
	<composite-id>
		<key-property name="physicallocationid" column="physical_location_id"  type="string"/>
		<key-property name="rackname" column="rack_name" type="string" />
	</composite-id>
	<property name="racktype" column="rack_type" type="string" />
	<property name="racklocation" column="rack_location" type="string" />
	<property name="rackposition" column="rack_position" type="string" />
	<property name="rackpowerdiversity" column="rack_power_diversity" type="string" />
	<property name="resourceversion" column="resource_version" type="string" />
	<property name="updatedOn" column="UPDATED_ON" type="string" />
     </class>
</hibernate-mapping>
