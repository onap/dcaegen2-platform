<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.att.vcc.inventorycollector.domain.NosServer" schema="dti" table="rt_nos_server">
        <meta attribute="class-description">This class contains the nos-server detail.</meta>
        <meta attribute="use-in-tostring">true</meta>
        <composite-id>
            <key-property name="cloudowner" column="CLOUD_OWNER" type="string"/>
            <key-property name="cloudregionid" column="CLOUD_REGION_ID" type="string"/>
            <key-property name="tenantid" column="TENANT_ID" type="string"/>
            <key-property name="nosserverid" column="NOS_SERVER_ID" type="string"/>
        </composite-id>
        <property name="nosservername" column="NOS_SERVER_NAME" type="string" />
        <property name="vendor" column="VENDOR" type="string" />
        <property name="provstatus" column="PROV_STATUS" type="string" />
        <property name="nosserverselflink" column="NOS_SERVER_SELFLINK" type="string" />
        <property name="inmaint" column="IN_MAINT" type="string" />
        <property name="resourceversion" column="RESOURCE_VERSION" type="string" />
        <property name="updatedOn" column="UPDATED_ON" type="string" />
    </class>
</hibernate-mapping>
