<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.att.vcc.inventorycollector.domain.Service" schema="dti" table="rt_service">
        <meta attribute="class-description">This class contains the service detail.</meta>
        <meta attribute="use-in-tostring">true</meta>
        <id name="serviceid" column="SERVICE_ID"  type="java.lang.String"/>
        <property name="servicedescription" column="SERVICE_DESCRIPTION" type="string" />
        <property name="serviceselflink" column="SERVICE_SELFLINK" type="string" />
        <property name="resourceversion" column="RESOURCE_VERSION" type="string" />
        <property name="updatedOn" column="UPDATED_ON" type="string" />
    </class>
</hibernate-mapping>
