<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.att.vcc.inventorycollector.domain.Tenant" schema="dti" table="rt_tenant">
        <meta attribute="class-description">This class contains the tenant detail.</meta>
        <meta attribute="use-in-tostring">true</meta>
        <composite-id>
		    <key-property name="tenantid" column="TENANT_ID" type="string"/>
		    <key-property name="cloudowner" column="CLOUD_OWNER" type="string"/>
		    <key-property name="cloudregionid" column="CLOUD_REGION_ID" type="string"/>
		</composite-id>
        <property name="tenantname" column="TENANT_NAME" type="string" />
        <property name="resourceversion" column="RESOURCE_VERSION" type="string" />
        <property name="tenantcontext" column="TENANT_CONTEXT" type="string" />
        <property name="updatedOn" column="UPDATED_ON" type="string" />
    </class>
</hibernate-mapping>
