<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.att.vcc.inventorycollector.domain.VplsPe"  schema="dti" table="rt_vpls_pe">
        <meta attribute="class-description">This class contains the vpls-pe detail.</meta>
        <meta attribute="use-in-tostring">true</meta>
        <id name="equipmentname" column="EQUIPMENT_NAME"  type="java.lang.String"/>
 		<property name="provstatus" column="PROV_STATUS" type="string" />
		<property name="ipv4oamaddress" column="IPV4_OAM_ADDRESS" type="string" />
		<property name="equipmentrole" column="EQUIPMENT_ROLE" type="string" />
		<property name="vlanidouter" column="VLAN_ID_OUTER" type="string" />
		<property name="resourceversion" column="RESOURCE_VERSION" type="string" />
		<property name="updatedOn" column="UPDATED_ON" type="string" />
	</class>
</hibernate-mapping>
