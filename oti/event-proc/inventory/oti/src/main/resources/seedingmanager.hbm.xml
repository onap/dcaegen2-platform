<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.att.vcc.inventorycollector.domain.SeedingManager" schema="dti" table="rt_seeding_manager">
        <meta attribute="class-description">This class contains the seeding manager detail</meta>
        <meta attribute="use-in-tostring">true</meta>
        <id name="hostname" column="hostname"  type="java.lang.String"/>
        <property name="icmpip" column="icmp_ip" type="string" />
        <property name="snmpip" column="snmp_ip" type="string" />
        <property name="communitystring" column="community_string" type="string" />
        <property name="snmpversion" column="snmp_version" type="string" />
        <property name="designtype" column="design_type" type="string" />
        <property name="location" column="location" type="string" />
        <property name="devicetype" column="device_type" type="string" />
        <property name="entitytype" column="entity_type" type="string" />
        <property name="functioncode" column="function_code" type="string" />
        <property name="outputobjects" column="output_objects" type="string" />
        <property name="devicechangetimestamp" column="device_change_timestamp" type="string" />
        <property name="icmpintervalclass" column="icmp_interval_class" type="string" />
        <property name="fmmibpollerintervalclass" column="fmmib_poller_interval_class" type="string" />
        <property name="changetype" column="change_type" type="string" />
        <property name="updatedOn" column="UPDATED_ON" type="string" />
      </class>
</hibernate-mapping>
