<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.att.vcc.inventorycollector.domain.OperationalEnvironment" schema="dti" table="rt_operational_environment">
        <meta attribute="class-description">This class contains the operational environment detail.</meta>
        <meta attribute="use-in-tostring">true</meta>
        <id name="operationalenvironmentid" column="OPERATIONAL_ENVIRONMENT_ID"  type="java.lang.String"/>
        <property name="operationalenvironmentname" column="OPERATIONAL_ENVIRONMENT_NAME" type="string" />
        <property name="operationalenvironmenttype" column="OPERATIONAL_ENVIRONMENT_TYPE" type="string" />
        <property name="operationalenvironmentnstatus" column="OPERATIONAL_ENVIRONMENT_STATUS" type="string" />
        <property name="tenantcontext" column="TENANT_CONTEXT" type="string" />
        <property name="workloadcontext" column="WORKLOAD_CONTEXT" type="string" />
        <property name="resourceversion" column="RESOURCE_VERSION" type="string" />
        <property name="updatedOn" column="UPDATED_ON" type="string" />
    </class>
</hibernate-mapping>
