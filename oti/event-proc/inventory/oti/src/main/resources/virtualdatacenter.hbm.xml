<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.att.vcc.inventorycollector.domain.Virtualdatacenter" schema="dti" table="rt_virtual_data_center">
        <meta attribute="class-description">This class contains the flavor detail.</meta>
        <meta attribute="use-in-tostring">true</meta>
        <id name="vdcid" column="VDC_ID"  type="java.lang.String"/>
        <property name="vdcname" column="VDC_NAME" type="string" />
        <property name="resourceversion" column="RESOURCE_VERSION" type="string" />
        <property name="updatedOn" column="UPDATED_ON" type="string" />
     </class>
</hibernate-mapping>
