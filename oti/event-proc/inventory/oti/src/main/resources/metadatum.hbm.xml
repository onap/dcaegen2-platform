<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.att.vcc.inventorycollector.domain.Metadatum" schema="dti" table="rt_metadatum">
        <meta attribute="class-description">This class contains the metadatum detail.</meta>
        <meta attribute="use-in-tostring">true</meta>
         <composite-id>
		    <key-property name="metaname" column="METANAME" type="string"/>
		    <key-property name="parententityid" column="PARENT_ENTITY_ID" type="string"/>
		</composite-id>
    	<property name="metaval" column="METAVAL" type="string" />
		<property name="resourceversion" column="RESOURCE_VERSION" type="string" />
		<property name="parententitytype" column="PARENT_ENTITY_TYPE" type="string" />
		<property name="updatedOn" column="UPDATED_ON" type="string" />
	</class>
</hibernate-mapping>
