<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.att.vcc.inventorycollector.domain.Vserver" schema="dti" table="rt_vserver">
        <meta attribute="class-description">This class contains the vserver detail.</meta>
        <meta attribute="use-in-tostring">true</meta>
        <composite-id>
            <key-property name="vserverid" column="VSERVER_ID" type="string"/>
            <key-property name="tenantid" column="TENANT_ID" type="string"/>
            <key-property name="cloudowner" column="CLOUD_OWNER" type="string"/>
            <key-property name="cloudregionid" column="CLOUD_REGION_ID" type="string"/>
        </composite-id>
        <property name="vservername" column="VSERVER_NAME" type="string" />
        <property name="vservername2" column="VSERVER_NAME2" type="string" />
        <property name="provstatus" column="PROV_STATUS" type="string" />
        <property name="vserverselflink" column="VSERVER_SELFLINK" type="string" />
        <property name="inmaint" column="IN_MAINT" type="string" />
        <property name="isclosedloopdisabled" column="IS_CLOSED_LOOP_DISABLED" type="string" />
        <property name="resourceversion" column="RESOURCE_VERSION" type="string" />
        <property name="updatedOn" column="UPDATED_ON" type="string" />
    </class>
</hibernate-mapping>
