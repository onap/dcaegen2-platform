<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.att.vcc.inventorycollector.domain.ForwardingPath" schema="dti" table="rt_forwarding_path">
        <meta attribute="class-description">This class contains the forwarding-path detail.</meta>
        <meta attribute="use-in-tostring">true</meta>
        <id name="forwardingPathId" column="FORWARDING_PATH_ID"  type="java.lang.String"/>
        <property name="forwardingPathName" column="FORWARDING_PATH_NAME" type="string" />
        <property name="selflink" column="SELFLINK" type="string" />
        <property name="resourceversion" column="RESOURCE_VERSION" type="string" />
        <property name="updatedOn" column="UPDATED_ON" type="string" />
    </class>
</hibernate-mapping>
