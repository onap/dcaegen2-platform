<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.att.vcc.inventorycollector.domain.VerticaFeed" schema="dti" table="vertica_feed">
        <meta attribute="class-description">This class contains the vetl/vertica feed detail</meta>
        <meta attribute="use-in-tostring">true</meta>
        <composite-id>
		    <key-property name="entitytype" column="entity_type" type="string"/>
		    <key-property name="action" column="action" type="string"/>
		    <key-property name="entityvalue" column="entity_value" type="string"/>
		    <key-property name="updatedOn" column="UPDATED_ON" type="string"/>
	     </composite-id>
    </class>
</hibernate-mapping>
