<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.att.vcc.inventorycollector.domain.Forwarder" schema="dti" table="rt_forwarder">
        <meta attribute="class-description">This class contains the forwarder detail.</meta>
        <meta attribute="use-in-tostring">true</meta>
        <composite-id>
          <key-property name="sequence" column="SEQUENCE"  type="java.lang.Integer"/>
          <key-property name="forwardingPathId" column="FORWARDING_PATH_ID"  type="java.lang.String"/>
        </composite-id>
        <property name="forwarderRole" column="FORWARDER_ROLE" type="string" />
        <property name="resourceversion" column="RESOURCE_VERSION" type="string" />
        <property name="updatedOn" column="UPDATED_ON" type="string" />
    </class>
</hibernate-mapping>