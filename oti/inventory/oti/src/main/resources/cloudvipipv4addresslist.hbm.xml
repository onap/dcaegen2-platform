<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.att.vcc.inventorycollector.domain.Cloudvipipv4addresslist"  schema="dti" table="rt_cloud_vip_ipv4_address_list">
        <meta attribute="class-description">This class contains the cloud vip ipv4 address list detail.</meta>
        <meta attribute="use-in-tostring">true</meta>
        <composite-id>
        <key-property name="vipipv4address" column="VIP_IPV4_ADDRESS" type="string"/>
        <key-property name="cloudowner" column="CLOUD_OWNER" type="string"/>
        <key-property name="cloudregionid" column="CLOUD_REGION_ID" type="string"/>
        </composite-id>
        <property name="vipipv4prefixlength" column="VIP_IPV4_PREFIX_LENGTH" type="java.lang.Long" />
        <property name="vlanidinner" column="VLAN_ID_INNER" type="java.lang.Long" />
        <property name="vlanidouter" column="VLAN_ID_OUTER" type="java.lang.Long" />
        <property name="isfloating" column="IS_FLOATING" type="string" />
    	<property name="resourceversion" column="RESOURCE_VERSION" type="string" />
    	<property name="neutronnetworkid" column="NEUTRON_NETWORK_ID" type="string" />
    	<property name="neutronsubnetid" column="NEUTRON_SUBNET_ID" type="string" />
        <property name="updatedOn" column="UPDATED_ON" type="string" />
    </class>
</hibernate-mapping>
