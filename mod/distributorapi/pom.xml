		<sonar.language>py</sonar.language>
		<sonar.pluginname>python</sonar.pluginname>
		<sonar.inclusions>**/*.py</sonar.inclusions>
		<sonar.exclusions>**/tests/**,**/setup.py</sonar.exclusions>
	</properties>
</project>
