+package org.apache.nifi.nar;
+
+import org.apache.nifi.bundle.Bundle;
+import org.slf4j.Logger;
+import org.slf4j.LoggerFactory;
+
+import java.io.IOException;
+import java.net.URI;
+import java.net.URL;
+import java.util.List;
+import java.util.Set;
+
+/**
+ * Starts a thread to monitor the auto-load directory for new DCAE jars
+ */
+public class DCAEAutoLoader {
+
+    private static final Logger LOGGER = LoggerFactory.getLogger(DCAEAutoLoader.class);
+
+    private static final long POLL_INTERVAL_MS = 5000;
+
+    private static class LoaderTask implements Runnable {
+
+        private static final Logger LOGGER = LoggerFactory.getLogger(LoaderTask.class);
+
+        private final URI indexJsonDcaeJars;
+        private final ExtensionDiscoveringManager extensionManager;
+        private volatile boolean stopped = false;
+
+        private LoaderTask(URI indexJsonDcaeJars, ExtensionDiscoveringManager extensionManager) {
+            this.indexJsonDcaeJars = indexJsonDcaeJars;
+            this.extensionManager = extensionManager;
+        }
+
+        @Override
+        public void run() {
+            while (!stopped) {
+                try {
+                    List<URL> urls = DCAEClassLoaders.getDCAEJarsURLs(this.indexJsonDcaeJars);
+                    Set<Bundle> bundles = DCAEClassLoaders.createDCAEBundles(urls);
+
+                    if (bundles.isEmpty()) {
+                        LOGGER.warn("MIKE: No DCAE bundles loaded");
+                    } else {
+                        this.extensionManager.discoverExtensions(bundles);
+                        // TODO: REMOVE
+                        this.stopped = true;
+                    }
+                } catch (final Exception e) {
+                    LOGGER.error("Error loading DCAE jars due to: " + e.getMessage(), e);
+                }
+
+                try {
+                    Thread.sleep(1000);
+                } catch (InterruptedException e) {
+                    e.printStackTrace();
+                }
+            }
+        }
+
+        public void stop() {
+            LOGGER.info("Stopping DCAE Auto-loader");
+            stopped = true;
+        }
+    }
+
+    private LoaderTask task;
+
+    public synchronized void start(URI indexJsonDcaeJars, final ExtensionDiscoveringManager extensionManager) throws IOException {
+        // Restricting to a single thread
+        if (task != null) {
+            return;
+        }
+
+        LOGGER.info("Starting DCAE Auto-Loader: {}", new Object[]{indexJsonDcaeJars});
+
+        this.task = new LoaderTask(indexJsonDcaeJars, extensionManager);
+        final Thread thread = new Thread(this.task);
+        thread.setName("DCAE Auto-Loader");
+        thread.setDaemon(true);
+        thread.start();
+
+        LOGGER.info("DCAE Auto-Loader started");
+    }
+
+    public synchronized void stop() {
+        this.task.stop();
+        this.task = null;
+        LOGGER.info("DCAE Auto-Loader stopped");
+    }
+
+}
