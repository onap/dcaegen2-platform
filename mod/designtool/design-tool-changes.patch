 import java.nio.charset.StandardCharsets;
@@ -142,6 +143,16 @@ public class NiFi {
 
         final Set<Bundle> narBundles = narClassLoaders.getBundles();
 
+        // Load DCAEProcessor bundles
+        try {
+            // TODO: Replace this hardcoding
+            URI indexJSON = new URI("https://nexus01.research.att.com:8443/repository/dcae-mod-raw/nifi-jars/index.json");
+            List<URL> jarURLsForDCAE = DCAEClassLoaders.getDCAEJarsURLs(indexJSON);
+            narBundles.addAll(DCAEClassLoaders.createDCAEBundles(jarURLsForDCAE));
+        } catch(Exception e) {
+            LOGGER.warn("DCAE loading issue! Report and move on", e);
+        }
+
         // load the server from the framework classloader
         Thread.currentThread().setContextClassLoader(frameworkClassLoader);
         Class<?> jettyServer = Class.forName("org.apache.nifi.web.server.JettyServer", true, frameworkClassLoader);
